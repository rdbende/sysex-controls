<?xml version="1.0" encoding="UTF-8"?>
<interface>
  <requires lib="gtk" version="4.0"/>
  <requires lib="libadwaita" version="1.0"/>
  <template class="Kl3FaderPage" parent="ScNavigationPage">
    <property name="title" translatable="yes">Faders</property>
    <property name="child">
      <object class="AdwToolbarView">
        <child type="top"><object class="AdwHeaderBar"/></child>
        <property name="content">
          <object class="ScPreferencesPage">

            <child>
              <object class="AdwPreferencesGroup">

                <child>
                  <object class="AdwSwitchRow">
                    <child><object class="ArControl"><property name="id">0x1401</property></object></child>
                    <property name="title" translatable="yes">Touch Sensitive</property>
                  </object>
                </child>

              </object>
            </child>

            <child>
              <object class="Kl3Fader">
                <property name="title" translatable="yes">Fader 1</property>
                <property name="control-id-offset">0x0000</property>
              </object>
            </child>

            <child>
              <object class="Kl3Fader">
                <property name="title" translatable="yes">Fader 2</property>
                <property name="control-id-offset">0x0800</property>
              </object>
            </child>

            <child>
              <object class="Kl3Fader">
                <property name="title" translatable="yes">Fader 3</property>
                <property name="control-id-offset">0x1000</property>
              </object>
            </child>

            <child>
              <object class="Kl3Fader">
                <property name="title" translatable="yes">Fader 4</property>
                <property name="control-id-offset">0x1800</property>
              </object>
            </child>

            <child>
              <object class="Kl3Fader">
                <property name="title" translatable="yes">Fader 5</property>
                <property name="control-id-offset">0x2000</property>
              </object>
            </child>

            <child>
              <object class="Kl3Fader">
                <property name="title" translatable="yes">Fader 6</property>
                <property name="control-id-offset">0x2800</property>
              </object>
            </child>

            <child>
              <object class="Kl3Fader">
                <property name="title" translatable="yes">Fader 7</property>
                <property name="control-id-offset">0x3000</property>
              </object>
            </child>

            <child>
              <object class="Kl3Fader">
                <property name="title" translatable="yes">Fader 8</property>
                <property name="control-id-offset">0x3800</property>
              </object>
            </child>

            <child>
              <object class="Kl3Fader">
                <property name="title" translatable="yes">Fader 9</property>
                <property name="control-id-offset">0x4000</property>
              </object>
            </child>

          </object>
        </property>
      </object>
    </property>

  </template>
</interface>
