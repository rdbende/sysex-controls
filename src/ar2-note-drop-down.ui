              <item>A3</item>
              <item>A#3</item>
              <item>B3</item>

              <item>C4</item>
              <item>C#4</item>
              <item>D4</item>
              <item>D#4</item>
              <item>E4</item>
              <item>F4</item>
              <item>F#4</item>
              <item>G4</item>
              <item>G#4</item>
              <item>A4</item>
              <item>A#4</item>
              <item>B4</item>

              <item>C5</item>
            </items>
          </object>
        </property>
      </object>
    </child>
  </template>
</interface>
