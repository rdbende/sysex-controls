<?xml version="1.0" encoding="UTF-8"?>
<interface>
  <requires lib="gtk" version="4.0"/>
  <requires lib="libadwaita" version="1.0"/>
  <template class="Ks37Knob" parent="ScPreferencesGroup">

    <child>
      <object class="ScCcRow">
        <child><object class="ScArturiaControl"><property name="id">0x00</property></object></child>
      </object>
    </child>

    <child>
      <object class="AdwSpinRow">
        <child><object class="ScArturiaControl"><property name="id">0x01</property></object></child>
        <property name="title" translatable="yes">Min</property>
        <property name="adjustment">
          <object class="GtkAdjustment">
            <property name="lower">0</property>
            <property name="upper">127</property>
            <property name="value">0</property>
            <property name="page-increment">10</property>
            <property name="step-increment">1</property>
          </object>
        </property>
      </object>
    </child>

    <child>
      <object class="AdwSpinRow" id="knob_max_range">
        <child><object class="ScArturiaControl"><property name="id">0x02</property></object></child>
        <property name="title" translatable="yes">Max</property>
        <property name="adjustment">
          <object class="GtkAdjustment">
            <property name="lower">0</property>
            <property name="upper">127</property>
            <property name="value">127</property>
            <property name="page-increment">10</property>
            <property name="step-increment">1</property>
          </object>
        </property>
      </object>
    </child>

    <child>
      <object class="ScComboRow">
        <child><object class="ScArturiaControl"><property name="id">0x03</property></object></child>
        <property name="title" translatable="yes">Channel</property>
        <property name="title-lines">1</property>
        <property name="model">
          <object class="ScControlValueList">
            <items>
              <item>1</item>
              <item>2</item>
              <item>3</item>
              <item>4</item>
              <item>5</item>
              <item>6</item>
              <item>7</item>
              <item>8</item>
              <item>9</item>
              <item>10</item>
              <item>11</item>
              <item>12</item>
              <item>13</item>
              <item>14</item>
              <item>15</item>
              <item>16</item>
              <item value="0x41">User</item>
            </items>
          </object>
        </property>
      </object>
    </child>

  </template>
</interface>
