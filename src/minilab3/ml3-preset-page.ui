<?xml version="1.0" encoding="UTF-8"?>
<interface>
  <requires lib="gtk" version="4.0"/>
  <requires lib="libadwaita" version="1.0"/>
  <template class="Ml3PresetPage" parent="ScNavigationPage">
    <property name="title" translatable="yes">Preset</property>
    <property name="child">
      <object class="AdwToolbarView">
        <child type="top"><object class="AdwHeaderBar"/></child>
        <property name="content">
          <object class="ScPreferencesPage">

            <child>
              <object class="ScPreferencesGroup">
                <property name="control-id-offset">0x08400000</property>
                <child>
                  <object class="ScComboRow">
                    <child><object class="ScArturiaControl"><property name="id">0x1300</property></object></child>
                    <property name="title" translatable="yes">Show on controller</property>
                    <property name="model">
                      <object class="ScControlValueList">
                        <items>
                          <item>Enabled</item>
                          <item>Disabled</item>
                        </items>
                      </object>
                    </property>
                  </object>
                </child>
              </object>
            </child>

            <child>
              <object class="AdwPreferencesGroup">

                <child>
                  <object class="AdwActionRow">
                    <property name="title" translatable="yes">Shift</property>
                    <property name="activatable">yes</property>
                    <property name="action-name">navigation.push</property>
                    <property name="action-target">'shift'</property>
                    <child type="suffix">
                      <object class="GtkImage">
                        <property name="icon-name">go-next-symbolic</property>
                      </object>
                    </child>
                  </object>
                </child>

                <child>
                  <object class="AdwActionRow">
                    <property name="title" translatable="yes">Pitch</property>
