<?xml version="1.0" encoding="UTF-8"?>
<interface>
  <requires lib="gtk" version="4.0"/>
  <requires lib="libadwaita" version="1.0"/>
  <template class="Ml3Book" parent="ScArturiaBook">
    <property name="child">
      <object class="AdwNavigationView">

        <child>
          <object class="Ml3GlobalPage">
            <property name="control-id-offset">0x08400000</property>
          </object>
        </child>

        <child>
          <object class="Ml3VelocityPage">
            <property name="control-id-offset">0x08400000</property>
          </object>
        </child>

        <child>
          <object class="Ml3PedalPage">
            <property name="control-id-offset">0x08400000</property>
          </object>
        </child>

        <child>
          <object class="Ml3PresetPage">
            <property name="title" translatable="yes">Preset 1</property>
            <property name="control-id-offset">0x0000</property>
          </object>
        </child>

        <child>
          <object class="Ml3PresetPage">
            <property name="title" translatable="yes">Preset 2</property>
            <property name="control-id-offset">0x0100</property>
          </object>
        </child>

        <child>
          <object class="Ml3PresetPage">
            <property name="title" translatable="yes">Preset 3</property>
            <property name="control-id-offset">0x0200</property>
          </object>
        </child>

        <child>
          <object class="Ml3PresetPage">
            <property name="title" translatable="yes">Preset 4</property>
            <property name="control-id-offset">0x0300</property>
          </object>
        </child>

        <child>
          <object class="Ml3PresetPage">
            <property name="title" translatable="yes">Preset 5</property>
            <property name="control-id-offset">0x0400</property>
          </object>
        </child>

      </object>
    </property>
  </template>
</interface>
