<?xml version="1.0" encoding="UTF-8"?>
<interface>
  <requires lib="gtk" version="4.0"/>
  <requires lib="libadwaita" version="1.0"/>
  <template class="Ml3MainKnob" parent="ScPreferencesGroup">

    <child>
      <object class="ScComboRow" id="control_output">
        <child><object class="ScArturiaControl"><property name="id">0x0e00</property></object></child>
        <property name="title" translatable="yes">Output</property>
        <property name="model">
          <object class="ScControlValueList">
            <items>
              <item>Off</item>
              <item>CC</item>
            </items>
          </object>
        </property>
      </object>
    </child>

    <child>
      <object class="ScComboRow">
        <child><object class="ScArturiaControl"><property name="id">0x1000</property></object></child>
        <property name="title" translatable="yes">Channel</property>
        <binding name="visible">
          <closure type='gboolean' function='cv_in'>
            <lookup type='ScComboRow' name='selected-item'>control_output</lookup>
            <constant type='GVariant'>[0x01]</constant>
          </closure>
        </binding>
        <property name="model">
          <object class="ScControlValueList">
            <items>
              <item>Keyboard</item>
              <item>1</item>
              <item>2</item>
              <item>3</item>
              <item>4</item>
              <item>5</item>
              <item>6</item>
              <item>7</item>
              <item>8</item>
              <item>9</item>
              <item>10</item>
              <item>11</item>
              <item>12</item>
              <item>13</item>
              <item>14</item>
              <item>15</item>
              <item>16</item>
            </items>
          </object>
        </property>
      </object>
    </child>

    <child>
      <object class="ScCcRow">
        <child><object class="ScArturiaControl"><property name="id">0x1200</property></object></child>
        <binding name="visible">
          <closure type='gboolean' function='cv_in'>
            <lookup type='ScComboRow' name='selected-item'>control_output</lookup>
            <constant type='GVariant'>[0x01]</constant>
          </closure>
        </binding>
      </object>
    </child>

  </template>
</interface>
