<?xml version="1.0" encoding="UTF-8"?>
<interface>
  <requires lib="gtk" version="4.0"/>
  <requires lib="libadwaita" version="1.0"/>
  <template class="Ml2Book" parent="ArBook">
    <property name="child">
      <object class="AdwNavigationView">

        <child>
          <object class="Ml2ControllerPage"/>
        </child>

        <child>
          <object class="Ml2VelocityPage"/>
        </child>

        <child>
          <object class="Ml2PitchModPage"/>
        </child>

        <child>
          <object class="Ml2PedalPage"/>
        </child>

        <child>
          <object class="Ml2KnobPage"/>
        </child>

        <child>
          <object class="Ml2KnobExtraPage"/>
        </child>

        <child>
          <object class="Ml2PadPage">
            <property name="title" translatable="yes">Pads 1-8</property>
            <property name="control-id-offset">0x00</property>
          </object>
        </child>

        <child>
          <object class="Ml2PadPage">
            <property name="title" translatable="yes">Pads 9-16</property>
            <property name="control-id-offset">0x08</property>
          </object>
        </child>

        <child>
          <object class="ArPresetPage">
            <property name="presets-num">8</property>
            <property name="readonly-num">1</property>
          </object>
        </child>

      </object>
    </property>
  </template>
</interface>
