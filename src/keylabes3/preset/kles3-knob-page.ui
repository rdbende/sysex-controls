<?xml version="1.0" encoding="UTF-8"?>
<interface>
  <requires lib="gtk" version="4.0"/>
  <requires lib="libadwaita" version="1.0"/>
  <template class="Kles3KnobPage" parent="ScNavigationPage">
    <property name="title" translatable="yes">Knobs</property>
