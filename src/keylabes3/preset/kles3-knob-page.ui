              </object>
            </child>

            <child>
              <object class="Kles3Knob">
                <property name="title" translatable="yes">Knob 8</property>
                <property name="control-id-offset">0x0700</property>
              </object>
            </child>

            <child>
              <object class="Kles3Knob">
                <property name="title" translatable="yes">Knob 9</property>
                <property name="control-id-offset">0x0800</property>
              </object>
            </child>

          </object>
        </property>
      </object>
    </property>

  </template>
</interface>
