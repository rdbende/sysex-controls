      <object class="AdwToolbarView">
        <child type="top"><object class="AdwHeaderBar"/></child>
        <property name="content">
          <object class="ScPreferencesPage">

            <child>
              <object class="Kles3Knob">
                <property name="title" translatable="yes">Knob 1</property>
                <property name="control-id-offset">0x0000</property>
              </object>
            </child>

            <child>
              <object class="Kles3Knob">
                <property name="title" translatable="yes">Knob 2</property>
                <property name="control-id-offset">0x0100</property>
              </object>
            </child>

            <child>
              <object class="Kles3Knob">
                <property name="title" translatable="yes">Knob 3</property>
                <property name="control-id-offset">0x0200</property>
              </object>
            </child>

            <child>
              <object class="Kles3Knob">
                <property name="title" translatable="yes">Knob 4</property>
                <property name="control-id-offset">0x0300</property>
              </object>
            </child>

            <child>
              <object class="Kles3Knob">
                <property name="title" translatable="yes">Knob 5</property>
